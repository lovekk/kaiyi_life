page{background-color: #f5f5f5;}
.top-banner{width: 100%; height: 375rpx;}
.img-wrapper swiper{width: 100%; height: 100%; position: relative;}
.slide-image{width: 100%; height: 100%;}
.swiper-box .wx-swiper-dots.wx-swiper-dots-horizontal{width: 100%; padding-right: 30rpx;display: flex;justify-content:flex-end;box-sizing: border-box;}
.img-hox{position: relative;}
.img-hox navigator{width: 100%; height: 375rpx;}
.text-bar{position: absolute; left: 0; bottom: 0; width: 100%; height: 70rpx; background-color: rgba(0,0,0,.5); color: #fff; font-size: 28rpx; display: flex; align-items: center;  padding: 0 30rpx; box-sizing: border-box;}
.text-bar .hot-txt{display: block; width: 75rpx; height: 38rpx; line-height: 38rpx; text-align: center; background-color: #fc3b3b; font-size: 25rpx; margin-right: 10rpx;}
.title{flex: 1; overflow: hidden;white-space: nowrap;text-overflow: ellipsis;}

.slider-hot{display: flex; background-color: #fff; height: 100rpx; margin:15rpx 0; padding: 0 18rpx; align-items: center; background-image: url('data:image/png;base64,'); background-size: 13rpx 19rpx; background-position: 96% center; background-repeat: no-repeat;}
.slider-hot .img-box{width: 118rpx; height: 29rpx; background-image: url('data:image/png;base64,'); background-size: 100% 100%; margin-right: 30rpx;}
.swiper_container {
  height: 50rpx;
  line-height: 50rpx;
  width: 540rpx;
}
.swiper_item {
  font-size: 30rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.list-wrapper,.list-item{width: 100%;}
.list-wrapper .list-item{background-color: #fff;  padding:0 20rpx; box-sizing: border-box;}
.list-wrapper .list-item navigator{width: 100%; justify-content: space-between; box-sizing: border-box; display: flex; border-bottom:1px solid #eee; padding:28rpx 0;}
.list-wrapper .list-item .text-box{width: 425rpx; display: flex; flex-direction: column; justify-content: space-around;}
.list-wrapper .list-item .text-box .title-bar{font-size: 28rpx; color: #282828; display: -webkit-box; display:-moz-box; display:box;-webkit-box-orient: vertical; -moz-box-orient: vertical; -webkit-line-clamp: 2;-moz-line-clamp: 2;overflow: hidden; line-height: 1.5;}
.list-wrapper .list-item .text-box .time-wrapper{display: flex; justify-content: space-between; font-size: 24rpx; color: #999999;}
.list-wrapper .list-item .text-box .time-wrapper .count{padding-left: 30rpx; background-image: url(data:image/png;base64,); background-size: 26rpx 20rpx; background-position: left center; background-repeat: no-repeat;}
.list-wrapper .list-item .img-box{width: 263rpx; height: 148rpx;}
.list-wrapper .list-item .img-box image{width: 263rpx; height: 148rpx;}
.loading{text-align:center;font-size:30rpx;}



