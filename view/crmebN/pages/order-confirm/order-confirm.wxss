.sh-address{position: relative; padding: 40rpx 20rpx 40rpx 76rpx; background-color: #fff; background-image: url('data:image/jpeg;base64,'); background-size: 100% auto; background-position: left bottom; background-repeat: no-repeat;}
.sh-address .iconfont{position: absolute; left: 20rpx; top: 50%; margin-top: -20rpx; font-size: 40rpx;}
.sh-address .text-wrapper{flex-direction: column; line-height: 2;}
.common-title{padding: 0 20rpx; height: 84rpx; line-height: 84rpx; border-bottom: 1px solid #efefef;}
.pro-list{margin-top: 20rpx; background-color: #fff; }
.pro-list .list-wrapper{padding: 20rpx;border-bottom: 1px solid #f7f7f7;}
.pro-list .list-wrapper .wrapper{padding: 0px 20rpx 0; background-color: #fafafa;}
.pro-list .list-wrapper .wrapper .item{position: relative; justify-content: space-between; border-bottom: 1px dashed #ddd; padding: 20rpx 0;}
.pro-list .list-wrapper .wrapper .item:last-child{border-bottom: 0;}
.pro-list .list-wrapper .wrapper .item image{width: 162rpx; height: 162rpx; }
.pro-list .list-wrapper .wrapper .item .pro-info{display: flex; width: 495rpx; flex-direction: column; justify-content: space-around;}
.pro-list .list-wrapper .wrapper .item .pro-info .title{overflow: hidden;white-space: nowrap;text-overflow: ellipsis;}
.pro-list .list-wrapper .wrapper .item .pro-info .res{overflow: hidden;white-space: nowrap;text-overflow: ellipsis; font-size: 26rpx; color: #999;}
.pro-list .list-wrapper .wrapper .item .pro-info .price{color: #ff3d3d; font-size: 32rpx;}
.pro-list .list-wrapper .wrapper .item .pro-info .price text{font-size: 24rpx;}
.pro-list .list-wrapper .wrapper .item .pro-info .price .count{color: #333;margin-left: 10rpx;}
.text-wrapper{padding: 0 20rpx;}
.text-item{ height: 98rpx; border-bottom: 1px solid #f7f7f7; justify-content: space-between; align-items: center;}
.text-item.inp-wrapper{justify-content: normal;}
.text-item .inp-btn{flex: 1; margin-left: 20rpx;}
.text-item .inp-btn input{width: 100%;}
.select-btn{font-size: 26rpx;color: #666;}
.select-btn:after{content: "";display: inline-block; width: 15rpx;height: 15rpx;border-left: 3rpx solid #bbb;border-bottom: 3rpx solid #bbb;transform: rotate(-135deg); margin-left: 10rpx;}
.select-btn text{color: #ff3d3d;}
.sale-btn{font-size: 26rpx;color: #666;}
.sale-btn:after{content: "";display: inline-block; width: 15rpx;height: 15rpx;border-left: 3rpx solid #bbb;border-bottom: 3rpx solid #bbb;transform: rotate(-135deg); margin-left: 10rpx;}
.all-price{padding:0 20rpx; height: 95rpx; line-height: 95rpx; text-align: right;}
.all-price text{color: #ff3d3d;}
.payment-select{margin-top: 20rpx; background-color: #fff; font-size: 26rpx;}
.payment-select .warapper{padding: 0 20rpx;}
.payment-select .warapper .item .radio-wrapper{width: 100%; height: 85rpx; display: flex; justify-content: space-between; align-items: center; border-bottom: 1px solid #efefef;}
.payment-select .warapper .left-wrapper{ align-items: center; color: #565656;}
.payment-select .warapper .left-wrapper .iconfont{font-size: 40rpx; margin-right: 15rpx;}
.icon-weixinzhifu{color: #41b035;}
.icon-yuezhifu{color: #fc992c;}
.icon-wallet{color: #31abe5;}
.icon-dianpu{color: #f23d26;}


radio .wx-radio-input{
   border-radius: 50%;
   width: 40rpx;
   height: 40rpx;
}
radio .wx-radio-input.wx-radio-input-checked{
   border: 1px solid #ff3d3d !important;
   background: #ff3d3d !important;
}
radio .wx-radio-input.wx-radio-input-checked::before{
   border-radius: 50%;
   width: 40rpx;
   height: 40rpx;
   line-height: 40rpx;
   text-align: center;
   font-size:30rpx;
   color:#fff;
   background: transparent;
   transform:translate(-50%, -50%) scale(1);
   -webkit-transform:translate(-50%, -50%) scale(1);
}
checkbox .wx-checkbox-input{border-radius: 50%;width:40rpx;height: 40rpx;}
.foot{position: fixed; left: 0; bottom: 0; width: 100%;  border-top: 1px solid #eee; background-color: #fff; justify-content: space-between; align-items: center;}
.foot .submit-btn{width: 275rpx; height: 100rpx; line-height: 100rpx; background-color: #ff3d3d; color: #fff; text-align: center; border-radius:0;
}
.foot .text{margin-left: 20rpx;}
.foot .text .price{color: #ff3d3d;}
