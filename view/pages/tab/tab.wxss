.nav{
    display: flex;
    justify-content: space-between;
    padding: 10rpx;
}
.nav view{
    width: 300rpx;
    height: 80rpx;
    line-height: 60rpx;
    text-align: center;
}
.now{
    border-bottom: 3rpx solid red;
}
.tishi{margin:20rpx 24rpx 0 24rpx;color:#6C6C6C;}

.header{width:100%;height:200rpx;background-color:#393A3E;text-align:center;position:relative;}
.header .money{font-size:42rpx;color:#7C4608;margin:110rpx 0 8rpx 0;}
.header .text{font-size:26rpx;color:#FBDE5B;}
.tip{width:350rpx;height:70rpx;background-color:green;text-align:center;line-height:70rpx;color:#ffffff;border-radius:35rpx;position:absolute;bottom:-35rpx;left:50%;margin-left:-157rpx;}
.exten-item{background-color:#ffffff;margin:20rpx 24rpx 0 24rpx;}
.exten-list{height:104rpx;line-height:104rpx;border-bottom:1rpx dashed #E2E2E2;padding:0 29rpx;justify-content: space-between;border-top-left-radius: 45%;position:relative;font-size:30rpx;color:#6C6C6C;}
.exten-list~.exten-list::after{width: 15px;height: 15px;content: "";right: -10px;top: -7px;position: absolute;display: block;background: #f5f5f5;border-radius: 50%;}
.exten-list~.exten-list::before{width: 15px;height: 15px;content: "";left: -10px;top: -7px;position: absolute;display: block;background: #f5f5f5;border-radius: 50%;}
.exten-money{font-size:28rpx;color:#6C6C6C;}
