.commcon-header{height:207rpx;background-color:#fff;padding:0 18rpx;justify-content:space-between;}
.commcon-pic{width:164rpx;height:164rpx;}
.commcon-pic image{width:100%;height:100%;}
.commcon-con{width:520rpx;height:164rpx;align-content:space-around;flex-wrap:wrap;}
.commcon-name{font-size:28rpx;color:#333333;word-break:break-all;display:-webkit-box;-webkit-line-clamp:1;-webkit-box-orient:vertical;overflow:hidden;}
.commcon-text{font-size:26rpx;color:#999999;}
.commcon-money{color:#FF3D3D;font-size:33rpx;}
.commcon-money text{font-size:24rpx;}
.commcon-num{color:#454545;margin-left:10rpx;}
 .commcon-con view{width:520rpx;} 
 .commcon-ping{margin-top:20rpx;width:100%;background-color:#fff;padding:10rpx 20rpx;}
 .placeholder{color:#B0B0B0;}
 .commcon-ping textarea{height:200rpx;width:710rpx;font-size:26rpx;padding-top:23rpx;}
 .upimg{background-image:url("data:image/png;base64,");background-repeat:no-repeat;background-size:100% 100%;width:162rpx;height:162rpx;display:inline-block;margin-bottom:20rpx;vertical-align:middle;}
 .commcon-ul{height:210rpx;background-color:#fff;margin-top:20rpx;padding:0 20rpx 0 20rpx;flex-wrap:wrap;align-content:center;}
 .commconxing icon{font-size:40rpx!important;margin-right:15rpx;color:#BABABA;}
 .commcon-li{line-height:65rpx;height:65rpx;}
 .li-lien{margin-right:27rpx;font-size:26rpx;color:#333333;}
 .li-lien3{font-size:24rpx;color:#FF7000;}
 .commcon-btn{width:710rpx;height:90rpx;line-height:90rpx;text-align:center;margin:50rpx auto;background-color:#FF7000;color:#fff;font-size:28rpx;border-radius:10rpx;}
 .icon-xingxing1{color:#FF7000!important;}
 .dataimg{width:100%!important;height:100%!important;}
 .dataimg:nth-of-type(4n){margin-right:0!important;}
 .imgpic{position:relative;width:162rpx;height:162rpx;display:inline-block;margin: 0 22rpx 20rpx 0;vertical-align:middle;}
 .guanbi{position:absolute;font-size:40rpx;color:#FF3D3D;top: -6rpx;right:0;}